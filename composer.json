{
	"require": {
		"laravel/framework": "5.0.*",
		"ikimea/browser": "*",
		"laracasts/presenter": "~0.2",
		"guzzlehttp/guzzle": "~4.2",
		"zizaco/entrust": "dev-laravel-5",
		"forxer/Gravatar": "*",
		
		"intervention/image": "*",
		"league/fractal": "*",
		"league/commonmark": "*",
		"laravelcollective/html": "~5.0"
	},
	
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"phpspec/phpspec": "~2.1",
		"fzaninotto/faker": "*",
		"barryvdh/laravel-debugbar": "~2"
	},
	
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"Help\\": "app/"
		}
	},

	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},

	"config": {
		"preferred-install": "dist",
		"process-timeout": 500
	},

	"minimum-stability": "dev",
	"prefer-stable": true
}
