{
	"require": {
		"laravel/framework": "~4.2.0",
		"erusev/parsedown": "~1.1.0",
		"erusev/parsedown-extra": "~0.2",
		"ikimea/browser": "~1.0",
		"laracasts/presenter": "~0.1.0",
		"guzzlehttp/guzzle": "~4.2.0",
		"zizaco/entrust": "~1.3.0",
		"forxer/Gravatar": "~1.2.0",
		
		"intervention/image": "~2.0.0",
		"league/flysystem": "~0.5.0",
		"league/fractal": "~0.9.0"
	},

	"require-dev": {
		"raveren/kint": "dev-master",
		"fzaninotto/faker": "~1.0",
		"barryvdh/laravel-debugbar": "~1.0"
	},

	"autoload": {
		"classmap": [
			"Anodyne/database/migrations",
			"Anodyne/database/seeds"
