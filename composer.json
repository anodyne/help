{
	"require": {
		"laravel/framework": "~5.2.0",
		"league/commonmark": "*",
		"laravelcollective/html": "~5.2.0",
		"ikimea/browser": "~1.0",
		"laracasts/presenter": "~0.2",
		"guzzlehttp/guzzle": "~4.2.0",
		"zizaco/entrust": "dev-laravel-5",
		"forxer/Gravatar": "~1.2",

		"intervention/image": "~2.0.0",
		"league/fractal": "*"
	},
	
	"require-dev": {
		"fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*",
		"barryvdh/laravel-debugbar": "~2.0"
	},
	
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"Help\\": "app/"
		}
	},
	
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	
	"config": {
		"preferred-install": "dist",
		"process-timeout": 500
	},

	"minimum-stability": "dev",
	"prefer-stable": true
}
